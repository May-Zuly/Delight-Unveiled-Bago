{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "total": {
      "type": "biginteger",
      "required": true,
      "min": "0"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "order",
        "delivering",
        "delivered"
      ],
      "default": "order",
      "required": true
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "purchases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::purchase.purchase",
      "mappedBy": "order"
    },
    "payment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::payment.payment",
      "mappedBy": "order"
    }
  }
}
